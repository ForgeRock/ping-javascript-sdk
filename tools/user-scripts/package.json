{
  "name": "@forgerock/pingone-scripts",
  "version": "0.0.0",
  "private": true,
  "description": "Scripts for interacting with PingOne",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "main": "./dist/index.js",
  "scripts": {
    "build": "pnpm nx nxBuild",
    "lint": "pnpm nx nxLint",
    "test": "pnpm nx nxTest",
    "test:watch": "pnpm nx nxTest --watch"
  },
  "overrides": {
    "@effect/platform": "^0.88.0",
    "@effect/platform-node": "^0.90.0",
    "@effect/vitest": "^0.24.0",
    "effect": "^3.16.0",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@effect/platform": "^0.88.0",
    "@effect/platform-node": "^0.90.0",
    "effect": "^3.16.0",
    "vitest": "3.2.4"
  },
  "devDependencies": {
    "@effect/language-service": "^0.26.0",
    "@effect/vitest": "^0.24.0",
    "vitest": "3.2.4"
  },
  "nx": {
    "tags": ["scope:tool"],
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": ["{options.outputPath}"],
        "options": {
          "outputPath": "dist",
          "main": "src/index.ts",
          "tsConfig": "tsconfig.app.json",
          "generatePackageJson": true
        }
      }
    }
  }
}
