{
  "name": "@forgerock/shared-types",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForgeRock/ping-javascript-sdk.git",
    "directory": "packages/shared-types"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": ["dist"],
  "dependencies": {
    "@reduxjs/toolkit": "catalog:",
    "tslib": "^2.3.0"
  },
  "nx": {
    "tags": ["scope:shared-types"],
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": ["{options.outputPath}"],
        "options": {
          "outputPath": "packages/shared-types/dist",
          "main": "packages/shared-types/src/index.ts",
          "tsConfig": "packages/shared-types/tsconfig.lib.json",
          "generatePackageJson": false,
          "assets": []
        }
      }
    }
  }
}
