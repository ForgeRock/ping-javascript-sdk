{
  "name": "@forgerock/sdk-utilities",
  "version": "2.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForgeRock/ping-javascript-sdk.git",
    "directory": "packages/sdk-utilities"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./constants": {
      "types": "./dist/src/lib/constants/index.d.ts",
      "import": "./dist/src/lib/constants/index.js",
      "default": "./dist/src/lib/constants/index.js"
    },
    "./package.json": "./package.json",
    "./types": {
      "types": "./dist/src/types.d.ts",
      "import": "./dist/src/types.js",
      "default": "./dist/src/types.js"
    }
  },
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "files": ["dist"],
  "scripts": {
    "build": "pnpm nx nxBuild",
    "lint": "pnpm nx nxLint",
    "test": "pnpm nx nxTest",
    "test:watch": "pnpm nx nxTest --watch"
  },
  "dependencies": {
    "@forgerock/sdk-types": "workspace:*"
  },
  "nx": {
    "tags": ["scope:sdk-utilities"]
  }
}
