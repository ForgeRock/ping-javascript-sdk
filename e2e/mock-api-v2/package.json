{
  "name": "@forgerock/mock-api-v2",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "type": "module",
  "main": "./src/main.js",
  "scripts": {
    "build": "pnpm nx nxBuild",
    "dev": "pnpm nx build --watch & node dist/src/main.js --watch-path=./",
    "lint": "pnpm nx nxLint",
    "serve": "node dist/src/main.js",
    "test": "pnpm nx nxTest"
  },
  "dependencies": {
    "@effect/language-service": "catalog:effect",
    "@effect/platform": "catalog:effect",
    "@effect/platform-node": "catalog:effect",
    "effect": "catalog:effect",
    "nanoid": "5.1.5"
  },
  "devDependencies": {
    "@effect/vitest": "catalog:effect"
  },
  "nx": {
    "tags": ["scope:e2e"],
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": ["{options.outputPath}"],
        "options": {
          "outputPath": "e2e/mock-api-v2/dist",
          "main": "e2e/mock-api-v2/src/main.ts",
          "tsConfig": "e2e/mock-api-v2/tsconfig.app.json",
          "generatePackageJson": true
        }
      }
    }
  }
}
